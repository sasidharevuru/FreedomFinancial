<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.freedom.model.Department" table="DEPARTMENT" catalog="freedomem1">
        <id name="departmentNumer" type="java.lang.Integer">
            <column name="Dnumber"/>
        </id>
        <property name="departmentName" type="string">
            <column name="Dname"/>
        </property>
        <property name="managerSSN" type="string">
            <column name="Mgr_ssn" sql-type="char(9)"/>
        </property>
    </class>
</hibernate-mapping>
